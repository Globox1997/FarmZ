{
	"schemaVersion": 1,
	"id": "farmz",
	"version": "${version}",
	"name": "FarmZ",
	"description": "A mod about farming related things",
	"authors": [
		"Globox_Z"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/farmz",
		"sources": "https://github.com/Globox1997/FarmZ",
		"issues": "https://github.com/Globox1997/FarmZ/issues"
	},
	"license": "MIT",
	"icon": "assets/farmz/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.farmz.FarmMain"
		],
		"client": [
			"net.farmz.FarmClient"
		],
		"modmenu": [
			"net.farmz.config.ModMenuIntegration"
		],
		"fabric-datagen": [
			"net.farmz.FarmDataGenerator"
		]
	},
	"mixins": [
		"farmz.mixins.json"
	],
	"depends": {
		"java": ">=17",
		"minecraft": ">=${minecraft_version}",
		"fabric-api": ">=${fabric_version}",
		"fabricloader": ">=${loader_version}",
		"cloth-config2": ">=${cloth_config_version}"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"mc-publish": {
			"modrinth": "aCmAOfjR",
			"curseforge": 1451993
		}
